{
  "manifest_version": 3,
  "name": "insights",
  "description": "search for IOC across multiple apis with a single query. threatco server required",
  "version": "1.07",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/app.js",
        "js/parser.js",
        "js/renderer.js"
      ],
      "type": "module"
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon64.png",
      "128": "icon128.png"
    }
  }
}
